 * @author NAREN
 */
public class Loops8 {
       public static void main(String args[]) {
       
        {
            for(int i=0; i<2; i++)//rows
            {
                for(int j=0; j<2; j++)//colums
                {
                    System.out.print("hai"+ " ");
                }
                System.out.println();
            }
         
        }
    }

}
    

