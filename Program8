/**
 *
 * @author NAREN
 */
public class Loops7 {
    
    public static void main(String args[]) {
       
        {
            for(int i=1; i<=5; i++)//rows
            {
                for(int j=1; j<=5; j++)//colums
                {
                    System.out.print("*");
                }
                System.out.println();
            }
         
        }
    }

}
    

